    "@react-navigation/native": "^6.0.2",
    "@supabase/supabase-js": "^2.39.8",
    "@tabler/icons-react-native": "^3.4.0",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "dayjs": "^1.11.10",
    "expo": "^51.0.8",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.18",
