    "expo-font": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.9",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.17",
    "expo-web-browser": "~13.0.3",
    "jsonwebtoken": "^9.0.2",
    "native-base": "^3.4.28",
    "react": "18.2.0",
